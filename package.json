{
  "name": "@technote-space/toc-generator",
  "version": "4.1.4",
  "description": "GitHub Action to generate TOC.",
  "keywords": [
    "github",
    "github actions",
    "toc"
  ],
  "homepage": "https://github.com/technote-space/toc-generator",
  "bugs": {
    "url": "https://github.com/technote-space/toc-generator/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/technote-space/toc-generator.git"
  },
  "license": "MIT",
  "author": {
    "name": "Technote",
    "email": "technote.space@gmail.com",
    "url": "https://technote.space"
  },
  "files": [
    "lib",
    "action.yml"
  ],
  "scripts": {
    "build": "tsc",
    "cover": "jest --coverage",
    "lint": "eslint 'src/**/*.ts' '__tests__/**/*.ts' --cache",
    "lint:fix": "eslint --fix 'src/**/*.ts' '__tests__/**/*.ts'",
    "release": "yarn release-ga --test",
    "test": "yarn lint && yarn cover",
    "update": "npm_config_yes=true npx npm-check-updates -u --timeout 100000 && yarn install && yarn upgrade && yarn audit",
    "postinstall": "[ -n \"$CI\" ] || [ ! -f node_modules/.bin/husky ] || husky install",
    "prepublishOnly": "[ -n \"$CI\" ] || [ ! -f node_modules/.bin/pinst ] || pinst --disable",
    "postpublish": "[ -n \"$CI\" ] || [ ! -f node_modules/.bin/pinst ] || pinst --enable"
  },
  "dependencies": {
    "@actions/core": "^1.4.0",
    "@technote-space/doctoc": "^2.4.11",
    "@technote-space/github-action-helper": "^5.2.13",
    "@technote-space/github-action-log-helper": "^0.1.33",
    "@technote-space/github-action-pr-helper": "^2.2.8",
    "fast-glob": "^3.2.7"
  },
  "devDependencies": {
    "@actions/github": "^5.0.0",
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@technote-space/github-action-test-helper": "^0.7.18",
    "@technote-space/release-github-actions-cli": "^1.8.6",
    "@types/jest": "^26.0.24",
    "@types/node": "^16.4.1",
    "@typescript-eslint/eslint-plugin": "^4.28.4",
    "@typescript-eslint/parser": "^4.28.4",
    "eslint": "^7.31.0",
    "husky": "^7.0.1",
    "jest": "^27.0.6",
    "jest-circus": "^27.0.6",
    "lint-staged": "^11.1.0",
    "nock": "^13.1.1",
    "pinst": "^2.1.6",
    "ts-jest": "^27.0.4",
    "typescript": "^4.3.5"
  },
  "publishConfig": {
    "access": "public"
  }
}
